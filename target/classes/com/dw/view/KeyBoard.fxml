<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox fx:id="keyBoardPane" stylesheets="@SearchTable.css" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.dw.controller.KeyBoardController">

    <FlowPane styleClass="searchPane">
        <children>
            <Button mnemonicParsing="false" text="清空" styleClass="clearAllButton" fx:id="clearBtn" onAction="#clearAll"/>
            <TextField fx:id="tableNumberTextField"/>
            <Button mnemonicParsing="false" text="←"  fx:id="deleteBtn" onAction="#deleteOne"/>
        </children>
    </FlowPane>
    <HBox fx:id="hbox">
        <FlowPane fx:id="abcPane">
            <FlowPane fx:id="abcrow1"></FlowPane>
            <FlowPane fx:id="abcrow2"></FlowPane>
            <FlowPane fx:id="abcrow3"></FlowPane>
            <FlowPane fx:id="bottomPane"></FlowPane>
        </FlowPane>
        <GridPane fx:id="numberPane">

        </GridPane>
    </HBox>
</VBox>
